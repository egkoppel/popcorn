set default = 0
set timeout = 10

menuentry "@CMAKE_PROJECT_NAME@ [Kernel: convolution @CMAKE_PROJECT_VERSION@ @HUGOS_ARCH@]" {
    multiboot2 /boot/hug.bin
	module2 /boot/initramfs.tar.gz initramfs
	boot
}

menuentry "@CMAKE_PROJECT_NAME@ [Kernel: convolution @CMAKE_PROJECT_VERSION@ @HUGOS_ARCH@] (DEBUG)" {
    multiboot2 /boot/hug.bin log_level=3
    module2 /boot/initramfs.tar.gz initramfs
    boot
}

menuentry "@CMAKE_PROJECT_NAME@ [Kernel: convolution @CMAKE_PROJECT_VERSION@ @HUGOS_ARCH@] (TRACE)" {
    multiboot2 /boot/hug.bin log_level=4
    module2 /boot/initramfs.tar.gz initramfs
    boot
}
